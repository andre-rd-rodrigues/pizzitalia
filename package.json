  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css:autoprefixer": "postcss -u autoprefixer -r css/*.css",
    "build:css": "npm run css:autoprefixer",
    "build:images": "imagemin assets/img/**/* --out-dir=assets/img",
    "watch:images": "onchange \"assets/img\" -- npm run build:images",
    "build:js": "webpack --mode=production",
    "watch:js": "onchange \"js\" -- webpack --mode=development",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange \"/build\" -- npm run build:html"
  },
