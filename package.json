    "simple-parallax-js": "^5.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "htmlnano": "^1.0.1",
    "imagemin-cli": "^7.0.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "posthtml": "^0.16.5",
    "posthtml-cli": "^0.10.0",
